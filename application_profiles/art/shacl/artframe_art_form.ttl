artframe_art_form:WorkForm
  rdf:type sh:NodeShape ;
  rdfs:label "Art Object" ;
  sh:or (
          [
            sh:targetClass bf:StillImage ;
          ]
          [
            sh:targetClass bf:Object ;
           ]
           [
            sh:targetClass bf:Text ;
           ]
        ) ;

# WorkTitlePropertyGroup

  sh:property artframe_art_form:WorkForm_arm_hasPreferredTitle ;
  sh:property artframe_art_form:WorkForm_bf_title ;

# WorkActivityPropertyGroup

  sh:property artframe_art_form:WorkForm_activity_hasActivity ;  

#CopyrightPropertyGroup

  sh:property artframe_art_form:WorkForm_bf_copyrightDate ;

# WorkSubjectGenreFormPropertyGroup

  sh:property artframe_art_form:WorkForm_arm_hasStylePeriod ;
  sh:property artframe_art_form:WorkForm_bf_subject ;
  sh:property artframe_art_form:WorkForm_bf_genreForm ;
  sh:property artframe_art_form:WorkForm_bf_classification ;

# WorkLanguageFormPropertyGroup

  sh:property artframe_art_form:WorkForm_bf_language ;

# WorkToInstancePropertyGroup
  
  sh:property artframe_art_form:WorkForm_bf_hasInstance ;

# WorkNotePropertyGroup
  
  sh:property artframe_art_form:WorkForm_bf_note ;

# WorkAwardReceiptPropertyGroup
  
