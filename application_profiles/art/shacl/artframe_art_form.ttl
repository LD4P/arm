 # ///// PROPERTY SHAPES FOR WorkTitlePropertyGroup START HERE. /////////////////////////////////////////////////////////////

artframe_art_form:WorkForm_arm_hasPreferredTitle 
  a sh:PropertyShape ;
  sh:path arm:hasPreferredTitle ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record PreferredTitles here; there can be only one preferred title per language." ;
  sh:group artframe_art_property_groups:WorkTitlePropertyGroup ;
  sh:name "Preferred title" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
artframe_art_form:WorkForm_bf_title
  a sh:PropertyShape ;
  sh:path bf:title ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
  sh:group artframe_art_property_groups:WorkTitlePropertyGroup ;
  sh:name "Other title" ;
  sh:nodeKind sh:IRI ;
  sh:order "1"^^xsd:decimal .


# ///// PROPERTY SHAPES FOR WorkActivityPropertyGroup START HERE. /////////////////////////////////////////////////////////////

artframe_art_form:WorkForm_activity_hasActivity 
  a sh:PropertyShape ;
  sh:path activity:hasActivity ;
  sh:name "Agent activity or role" ;
  sh:order "0"^^xsd:decimal ;
  sh:group artframe_art_property_groups:WorkActivityPropertyGroup ;
  tosh:editWidget swa:NestedObjectEditor ; 
  sh:or ( 
          [
            activity:AuthorActivity 
          ]
          [
            activity:CreatorActivity 
          ]
          [
            activity:ArchitectActivity
          ]
          [
            activity:ArtistActivity 
          ]
          [
            activity:DesignerActivity 
          ]
          [  
            activity:DraftsmanActivity
          ]
          [
            activity:PhotographerActivity 
          ]
          [
            activity:SculptorActivity 
          ]  
         ) .
  
# ///// PROPERTY SHAPES FOR WorkCopyrightPropertyGroup START HERE. /////////////////////////////////////////////////////////////
  
 artframe_art_form:WorkForm_bf_copyrightDate 
  a sh:PropertyShape ;
  sh:path bf:copyrightDate ;
  sh:name "Copyright date" ;
  sh:order "0"^^xsd:decimal ;
  sh:group artframe_art_property_groups:WorkCopyrightPropertyGroup ;
  sh:nodeKind sh:Literal .
   
# ///// PROPERTY SHAPES FOR WorkSubjectGenreFormPropertyGroup START HERE. /////////////////////////////////////////////////////



artframe_art_form:WorkForm_bf_subject
      a sh:PropertyShape ;
      sh:path bf:subject ;
      sh:group artframe_art_property_groups:WorkSubjectGenreFormPropertyGroup ;
      sh:name "Subject" ;
      sh:nodeKind sh:IRI ;
      sh:order "0"^^xsd:decimal ;
      artframe_art_form:lookup artframe_art_form:LD4LookupService ;
      sh:or (
          [
            sh:class bf:Work ;
          ]
          [
            sh:class bf:Instance ;
          ]
          [
            sh:class bf:Item ;
          ]
          [
            sh:class bf:Temporal ;
          ]
          [
            sh:class bf:Agent ;
          ]
          [
            sh:class lingvo:Lingvo ;
          ]
          [
            sh:class skos:Concept ;
          ]
          [
            sh:class gn:Feature ;
          ]
          [
            sh:class bf:Event ;
          ]
        ) .

artframe_art_form:WorkForm_bf_genreForm 
  a sh:PropertyShape ;
  sh:path bf:genreForm ;
  sh:class skos:Concept ;
  sh:group artframe_art_property_groups:WorkSubjectGenreFormPropertyGroup ;
  sh:name "Form Genre" ;
  sh:nodeKind sh:IRI ;
  sh:order "1"^^xsd:decimal ;
  artframe_art_form:lookup artframe_art_form:LD4LookupService .
  
# The following sh:Property with sh:path bf:genreForm/madsrdf:isMemberOfMADSScheme isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 
  
artframe_art_form:WorkForm_bf_genreForm 
 a sh:PropertyShape ;
 sh:path <bf:genreForm/skos:inScheme> ;
 sh:description "Values for form genre terms" ;
 sh:group artframe_art_property_groups:WorkSubjectGenreFormPropertyGroup ;
 sh:name "Form Genre Terms" ;
 sh:nodeKind sh:IRI ;
 sh:or (
 [
 sh:hasValue http://id.loc.gov/authorities/genreForms ;
 ]
 [
 sh:hasValue <http://vocab.getty.edu/aat/> ;
 ]) ;
 skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." .
 
