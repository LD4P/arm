artframe_art_form:WorkForm_arm_hasPreferredTitle sh:path arm:hasPreferredTitle ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record PreferredTitles here; there can be only one preferred title per language." ;
  sh:group artframe_art_property_groups:WorkTitlePropertyGroup ;
  sh:name "Preferred title" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
artframe_art_form:WorkForm_bf_title sh:path bf:title ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
  sh:group artframe_art_property_groups:WorkTitlePropertyGroup ;
  sh:name "Other title" ;
  sh:nodeKind sh:IRI ;
  sh:order "1"^^xsd:decimal .


# ///// PROPERTY SHAPES FOR WorkActivityPropertyGroup START HERE. /////////////////////////////////////////////////////////////

artframe_art_form:WorkForm_activity_hasActivity sh:path activity:hasActivity ;
  sh:name "Agent activity or role" ;
  sh:order "0"^^xsd:decimal ;
  sh:group artframe_art_property_groups:WorkActivityPropertyGroup ;
  tosh:editWidget swa:NestedObjectEditor ; 
  sh:class ( activity:AuthorActivity activity:CreatorActivity activity:ArchitectActivity activity:ArtistActivity activity:DesignerActivity activity:DraftsmanActivity activity:PhotographerActivity activity:SculptorActivity ) .
  
