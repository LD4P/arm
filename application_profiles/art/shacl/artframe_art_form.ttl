# ///// PROPERTY SHAPES FOR WorkNotePropertyGroup START HERE. /////////////////////////////////////////////////////////////////////////

artframe_art_form:WorkForm_bf_note
      a sh:PropertyShape ;
      sh:path bf:note ;
      sh:description "Work related notes for when more granualar information isn't captured." ;
      sh:name "Work notes" ;
      tosh:editWidget swa:NestedObjectEditor ;
      sh:group artframe_art_property_groups:WorkNotePropertyGroup ;
      sh:or (
         
         [
           sh:class bf:Note ;
         ]
         [
           sh:class arm:DescriptiveNote ;
         ]
         [
           sh:class arm:SubjectNote ;
         ]
         
        ) ;
 
      sh:order "0"^^xsd:decimal .
 
    
