raremat_monograph_form:InstanceForm
  rdf:type sh:NodeShape ;
  rdfs:label "Instance of Monographic Text" ;
  sh:targetClass bf:Instance ;
  sh:property raremat_monograph_form:InstanceForm_arm_hasPreferredTitle ;
  sh:property raremat_monograph_form:InstanceForm_bf_title ;
  sh:property raremat_monograph_form:InstanceForm_bf_carrier ;
  sh:property raremat_monograph_form:InstanceForm_bf_genreForm ;
  sh:property raremat_monograph_form:InstanceForm_activity_hasActivity ;
  sh:property raremat_monograph_form:InstanceForm_bf_responsibilityStatement ;
  sh:property raremat_monograph_form:InstanceForm_bf_provisionActivityStatement ;
  sh:property raremat_monograph_form:InstanceForm_bf_hasPart ;
  sh:property raremat_monograph_form:InstanceForm_bf_accompaniedBy ;
  sh:property raremat_monograph_form:InstanceForm_bf_accompanies ;
  sh:property raremat_monograph_form:InstanceForm_bf_extent ;
  sh:property raremat_monograph_form:InstanceForm_measure_hasMeasurementGroup ;
  sh:property raremat_monograph_form:InstanceForm_bf_bookFormat ;
  sh:property raremat_monograph_form:InstanceForm_measure_hasSignatureStatement ;
  sh:property raremat_monograph_form:InstanceForm_measure_hasMaterial ;
  sh:property raremat_monograph_form:InstanceForm_rdau_P60596 ;
  sh:property raremat_monograph_form:InstanceForm_rdau_P60159 ;
  sh:property raremat_monograph_form:InstanceForm_arm_writtenIn ;
  sh:property raremat_monograph_form:InstanceForm_arm_markedBy ;
  sh:property raremat_monograph_form:InstanceForm_bf_instanceOf ;
  sh:property raremat_monograph_form:InstanceForm_bf_editionStatement ;
  sh:property raremat_monograph_form:InstanceForm_bf_hasItem ;
  sh:property raremat_monograph_form:InstanceForm_arm_hasCitation ;
  sh:property raremat_monograph_form:InstanceForm_award_receives ;
  sh:property raremat_monograph_form:InstanceForm_bf_note ;
  sh:property raremat_monograph_form:InstanceForm_bf_identifiedBy .


raremat_monograph_form:InstanceForm_arm_hasPreferredTitle sh:path arm:hasPreferredTitle ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record PreferredTitles here; there can be only one preferred title per language." ;
  sh:group raremat_monograph_property_groups:InstanceTitlePropertyGroup ;
  sh:name "Preferred title" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
raremat_monograph_form:InstanceForm_bf_title sh:path bf:title ;
  sh:class bf:Title ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
  sh:group raremat_monograph_property_groups:InstanceTitlePropertyGroup ;
  sh:name "Other title" ;
  sh:nodeKind sh:IRI ;
  sh:order "1"^^xsd:decimal .

raremat_monograph_form:InstanceForm_bf_carrier sh:path bf:carrier ;
  sh:class skos:Concept ;
  sh:in ( <http://id.loc.gov/vocabulary/carriers/nc> <http://id.loc.gov/vocabulary/carriers/nb> <http://id.loc.gov/vocabulary/carriers/na> <http://id.loc.gov/vocabulary/carriers/no> <http://id.loc.gov/vocabulary/carriers/nz>) ;
  sh:description "Carrier type" ;
  sh:group raremat_monograph_property_groups:InstanceTypePropertyGroup ;
  sh:name "Carrier type" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
raremat_monograph_form:InstanceForm_bf_genreForm sh:path bf:genreForm ;
  sh:class skos:Concept ;
  sh:group raremat_monograph_property_groups:InstanceGenreFormPropertyGroup ;
  sh:name "Form Genre" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal ;
  raremat_monograph_form:lookup raremat_monograph_form:LD4LookupService .
  
# The following sh:Property with sh:path bf:genreForm/madsrdf:isMemberOfMADSScheme isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 
  
raremat_monograph_form:InstanceForm_bf_genreForm_madsrdf_isMemberOfMADSScheme sh:path <bf:genreForm/madsrdf:isMemberOfMADSScheme> ;
 sh:hasValue <http://id.loc.gov/authorities/genreForms> ;
 sh:description "Values for form genre terms" ;
 sh:group raremat_monograph_property_groups:InstanceGenreFormPropertyGroup ;
 sh:name "LCGFT Form Genre Terms" ;
 sh:nodeKind sh:IRI ;
 skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." .
 

