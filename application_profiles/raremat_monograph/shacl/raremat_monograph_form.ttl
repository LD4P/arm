raremat_monograph_form:InstanceForm_activity_hasActivity sh:path activity:hasActivity ;
  sh:name "Publisher, Distributor, etc. activity or role" ;
  sh:description "Think MARC field 260 or 264" ;
  sh:order "0"^^xsd:decimal ;
  sh:group raremat_monograph_property_groups:InstanceActivityPropertyGroup ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:or ( 
          [
            sh:class activity:PublisherActivity  ;
          ]
          [
            sh:class activity:DistributorActivity ;
          ]
          [
            sh:class activity:PrinterActivity ;
          ]
          [
            sh:class activity:DesignerActivity ;
          ]
          [
            sh:class activity:ManufacturerActivity ;
          ]
          [
            sh:class activity:BinderActivity ;
          ]
          [
            sh:class activity:Activity 
          ]  
         ) .

