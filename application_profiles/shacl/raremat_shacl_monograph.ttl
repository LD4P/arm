raremat_shapes_monograph:InstanceForm rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:label "Instance of Monographic Text" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:targetClass bf:Instance ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasPreferredTitle ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_title ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_carrier ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_genreForm ;
  sh:property raremat_shapes_monograph:InstanceForm_arm_hasActivity ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_responsibilityStatement ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_provisionActivityStatement ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_hasPart ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_accompaniedBy ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_accompanies ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_extent ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasMeasurementGroup ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasSignatureStatement ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasMaterial ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasBaseMaterial ;
  sh:property raremat_shapes_monograph:InstanceForm_ex_hasAppliedMaterial ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_note ;
  sh:property raremat_shapes_monograph:InstanceForm_bf_identifiedBy .

# TODO Review. The following PropertyShape is subject to change depending on whether we follow BIBFRAME or bibliotek-o/ArtFrame. Going to stop work on this until a decision is made.

raremat_shapes_monograph:InstanceForm_ex_hasPreferredTitle sh:path <http://example.org/hasPreferredTitle> ;
  sh:class <http://id.loc.gov/ontologies/bibframe/Title> ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record PreferredTitles here; there can be only one preferred title per language." ;
  sh:group raremat_shapes_monograph:InstanceTitlePropertyGroup ;
  sh:name "Preferred title" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
raremat_shapes_monograph:InstanceForm_bf_title sh:path bf:title ;
  sh:class <http://id.loc.gov/ontologies/bibframe/Title> ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
  sh:group raremat_shapes_monograph:InstanceTitlePropertyGroup ;
  sh:name "Other title" ;
  sh:nodeKind sh:IRI ;
  sh:order "1"^^xsd:decimal .

raremat_shapes_monograph:InstanceForm_bf_carrier sh:path <http://id.loc.gov/ontologies/bibframe/carrier> ;
  sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
  sh:in ( <http://id.loc.gov/vocabulary/carriers/nc> <http://id.loc.gov/vocabulary/carriers/nb> <http://id.loc.gov/vocabulary/carriers/na> <http://id.loc.gov/vocabulary/carriers/no> <http://id.loc.gov/vocabulary/carriers/nz>) ;
  sh:description "Carrier type" ;
  sh:group raremat_shapes_monograph:InstanceTypePropertyGroup ;
  sh:name "Carrier type" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
raremat_shapes_monograph:InstanceForm_bf_genreForm sh:path bf:genreForm ;
  sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
  sh:group raremat_shapes_monograph:InstanceGenreFormPropertyGroup ;
  sh:name "Form Genre" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal ;
  raremat_shapes_monograph:lookup raremat_shapes_monograph:LD4LookupService .
  
# The following sh:Property with sh:path bf:genreForm/madsrdf:isMemberOfMADSScheme isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 
  
raremat_shapes_monograph:InstanceForm_bf_genreForm_madsrdf_isMemberOfMADSScheme sh:path <bf:genreForm/madsrdf:isMemberOfMADSScheme> ;
 sh:hasValue <http://id.loc.gov/authorities/genreForms> ;
 sh:description "Values for form genre terms" ;
 sh:group raremat_shapes_monograph:InstanceGenreFormPropertyGroup ;
 sh:name "LCGFT Form Genre Terms" ;
 sh:nodeKind sh:IRI ;
 skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." .
 

