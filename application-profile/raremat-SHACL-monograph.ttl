# TODO annotation properties and imports for http://example.org/shapes/monograph/

<http://example.org/shapes/monograph/>
  rdf:type owl:Ontology .

raremat_shapes_monograph:WorkForm
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:label "Monographic Text" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:targetClass bf:Text ;
  sh:property raremat_shapes_monograph:WorkForm_bf_title ;
  sh:property raremat_shapes_monograph:WorkForm_bf_genreForm ;
  sh:property raremat_shapes_monograph:WorkForm_bf_genreForm_madsrdf_isMemberOfMADSScheme ;
  sh:property raremat_shapes_monograph:WorkForm_raremat_hasActivity ;
  sh:property raremat_shapes_monograph:WorkForm_bf_hasPart ;
  sh:property raremat_shapes_monograph:WorkForm_bf_partOf ;
  sh:property raremat_shapes_monograph:WorkForm_bf_hasExpression ;
# TODO finish WorkToWork skolemization  
.

# The following PropertyShape is subject to change depending on whether we follow BIBFRAME or bibliotek-o/ArtFrame. Going to stop work on this until a decision is made.

raremat_shapes_monograph:WorkForm_bftitle sh:path <http://id.loc.gov/ontologies/bibframe/title> ;
  sh:class <http://id.loc.gov/ontologies/bibframe/Title> ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
  sh:group raremat_shapes_monograph:TitlePropertyGroup ;
  sh:name "Other title" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal .
  
raremat_shapes_monograph:WorkForm_raremat_hasActivity sh:path raremat:hasActivity ;
  sh:name "Agent activity or role" ;
  sh:order "0"^^xsd:decimal ;
  sh:group raremat_shapes_monograph:WorkActivityPropertyGroup ;
  tosh:editWidget swa:NestedObjectEditor ;
  sh:class raremat:Activity .
      
raremat_shapes_monograph:WorkForm_bfgenreForm sh:path bf:genreForm ;
  sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
  sh:group raremat_shapes_monograph:WorkSubjectGenreFormPropertyGroup ;
  sh:name "Form Genre" ;
  sh:nodeKind sh:IRI ;
  sh:order "0"^^xsd:decimal ;
  raremat_shapes_monograph:lookup raremat_shapes_monograph:LD4LookupService .
  
# The following sh:Property with sh:path bf:genreForm/madsrdf:isMemberOfMADSScheme isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 
  
raremat_shapes_monograph:WorkForm_bf_genreForm_madsrdf_isMemberOfMADSScheme sh:path <bf:genreForm/madsrdf:isMemberOfMADSScheme> ;
 sh:hasValue <http://id.loc.gov/authorities/genreForms> ;
 sh:description "Values for form genre terms" ;
 sh:group raremat_shapes_monograph:WorkSubjectGenreFormPropertyGroup ;
 sh:name "LCGFT Form Genre Terms" ;
 sh:nodeKind sh:IRI ;
 skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." .

# TODO decide what types of parts bf:Works a bf:Text can have, e.g. bf:StillImage

raremat_shapes_monograph:WorkForm_bf_hasPart sh:path <http://id.loc.gov/ontologies/bibframe/hasPart> ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
      sh:group raremat_shapes_monograph:WorkToWorkPropertyGroup ;
      sh:name "Contents of Monograph" .

# TODO decide what types of bf:Works a bf:Text can be a part of, e.g. another bf:Text

raremat_shapes_monograph:WorkForm_bf_partOf sh:path <http://id.loc.gov/ontologies/bibframe/partOf> ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
      sh:group raremat_shapes_monograph:WorkToWorkPropertyGroup ;
      sh:name "Is part or contents of" .

raremat_shapes_monograph:WorkForm_bf_hasExpression sh:path <http://id.loc.gov/ontologies/bibframe/hasExpression> ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
      sh:description "Work that is an expression of a described Work. For use to relate Works under FRBR/RDA rules." ;
      sh:group raremat_shapes_monograph::WorkToWorkPropertyGroup ;
      sh:name "Has expression" .

raremat_shapes_monograph:WorkForm_bf_referencedBy sh:path <http://id.loc.gov/ontologies/bibframe/referencedBy> ;
      sh:class <http://id.loc.gov/ontologies/bibframe/Work> ;
      sh:description "Resource that references the described resource." ;
      sh:name "Referenced by" ;
      sh:group raremat_shapes_monograph::WorkToWorkPropertyGroup .

